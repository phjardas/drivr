  "author": "Philipp Jardas <philipp@jardas.de>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "dist": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "firebase": "firebase",
    "deploy": "npm run deploy:hosting && npm run deploy:functions",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "npm run dist && firebase deploy --only hosting",
    "backup": "ts-node scripts/backup.ts",
    "postinstall": "(cd functions && npm install)"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.7.1",
    "dot-prop": "^4.2.0",
    "firebase": "^4.8.1",
    "vue": "^2.5.11",
    "vue-chartjs": "^3.0.2",
    "vue-moment": "^3.1.0",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.6.1",
    "vuetify": "^0.17.6",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@types/webpack-env": "^1.13.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "firebase-admin": "^5.6.0",
    "firebase-tools": "^3.16.0",
    "html-minifier": "^3.5.8",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.1"
  }
}
